{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "rust: cargo build debug",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe_rs Executable Test",
            "chip": "nRF52840_xxAA",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": true,
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "${workspaceFolder}/nrf/target/thumbv7em-none-eabihf/release/memory",
                    "svdFile": "${workspaceFolder}/nrf/hw/nrf52840.svd",
                    "rttEnabled": true,
                }
            ],
            "consoleLogLevel": "Info", //Error, Warn, Info, Debug, Trace
            "wireProtocol": "Swd"
        }
    ]
}